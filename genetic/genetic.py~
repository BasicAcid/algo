def populate():
    """Generate random list of 15 cities"""
    initial_solution = []
    for each in range(15):
        initial_solution.append(random.randrange(1, 15))
    return initial_solution

def rand_population():
    """Generate a list of lists :)"""
    initial_population = []
    for line in range(200):
        rand = populate()
        initial_population.append(rand)
    return initial_population
